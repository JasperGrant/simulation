  <model name="lunar_roving_vehicle">
            <static>false</static>
            
            
            <!-- Chassis -->
            <link name='chassis'>   
                <pose relative_to='__model__'>0 0 0 0 0 0</pose> 
                <inertial> <!--inertial properties of the link mass, inertia matix-->
                    <mass>210.0</mass>
                    <inertia>
                        <ixx>0.095329</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.381317</iyy>
                        <iyz>0</iyz>
                        <izz>0.476646</izz>
                    </inertia>
                </inertial>
                <visual name="visual">
                    <geometry>
                        <mesh>
                        <uri>model://lunar_roving_vehicle/meshes/lunar_roving_vehicle.dae</uri>
                        <scale>1.0 1.0 1.0</scale>
                        </mesh>
                    </geometry>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <mesh>
                        <uri>model://lunar_roving_vehicle/meshes/lunar_roving_vehicle.dae</uri>
                        <scale>1.0 1.0 1.0</scale>
                        </mesh>
                    </geometry>
                </collision>
            </link>

            <!-- Left Rear Wheel Rotator -->
            <link name='left_rear_steer'> 
                <pose relative_to="chassis">-1.4 0.914 0.6 0 0 0</pose> <!--angles are in radian-->
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
                        <izz>0.08</izz>
                    </inertia>
