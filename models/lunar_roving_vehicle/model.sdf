            <joint name='left_front_steer_joint' type='revolute'>
                <pose relative_to='left_front_steer'/>
                <parent>chassis</parent>
                <child>left_front_steer</child>
                <axis>
                    <xyz expressed_in='__model__'>0 0 1</xyz> <!--can be descired to any frame or even arbitrary frames-->
                    <limit>
                        <lower>-0.3</lower>    <!--negative infinity-->
                        <upper>0.3</upper>     <!--positive infinity-->
                    </limit>
                </axis>

            </joint>

